  54:  JMP LBL[1] ;
  55:   ;
  56:  LBL[9994:GRIPPER FAULT] ;
  57:  RO[1:T1 OPEN]=OFF ;
  58:  RO[2:T1 CLOSE]=ON ;
  59:  WAIT    .25(sec) ;
  60:  IF DO[36:PART PRESENT CHECK ONLY]=ON,JMP LBL[9999] ;
  61:  CALL TOOL_MSG(7) ;
  62:  UALM[1] ;
  63:  JMP LBL[1] ;
  64:   ;
